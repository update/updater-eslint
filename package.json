{
  "name": "updater-eslint",
  "description": "Update a `.eslintrc.json` file based on a template and preferences. This updater can be used from the command line when installed globally, or as a plugin in other updaters.",
  "version": "0.1.1",
  "homepage": "https://github.com/update/updater-eslint",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "update/updater-eslint",
  "bugs": {
    "url": "https://github.com/jonschlinkert/updater-eslint/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "README.md",
    "templates"
  ],
  "main": "index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">=5.0.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "delete": "^0.3.0",
    "generate-eslint": "^0.1.2",
    "is-valid-app": "^0.1.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3",
    "update": "^0.6.2",
    "verb-readme-generator": "^0.1.18"
  },
  "keywords": [
    "eslint",
    "fix",
    "lint",
    "linter",
    "maintain",
    "plugin",
    "plugins",
    "up-to-date",
    "update",
    "update-plugin",
    "update-updater",
    "updater",
    "updaterplugin"
  ],
  "verb": {
    "related": {
      "list": [
        "assemble",
        "update",
        "verb"
      ]
    },
    "toc": true,
    "layout": "common-minimal",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "generate-eslint",
      "update",
      "verb",
      "verb-readme-generator"
    ]
  }
}
