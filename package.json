{
  "name": "updater-eslint",
  "description": "Update a `.eslintrc.json` file based on a template and preferences. This updater can be used from the command line when installed globally, or as a plugin in other updaters.",
  "version": "1.0.0",
  "homepage": "https://github.com/update/updater-eslint",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)",
    "Stefan Walther (http://qliksite.io)"
  ],
  "repository": "update/updater-eslint",
  "bugs": {
    "url": "https://github.com/update/updater-eslint/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">=5"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "delete": "^1.1.0",
    "generate-eslint": "^1.0.0",
    "is-valid-app": "^0.3.0"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.4.2",
    "update": "^0.7.4"
  },
  "keywords": [
    "eslint",
    "fix",
    "lint",
    "linter",
    "maintain",
    "maintainance",
    "plugin",
    "plugins",
    "up-to-date",
    "update",
    "update-plugin",
    "update-updater",
    "updateplugin",
    "updater",
    "updaterplugin",
    "upkeep"
  ],
  "verb": {
    "toc": {
      "render": true,
      "method": "preWrite",
      "collapsible": true,
      "maxdepth": 4
    },
    "related": {
      "list": [
        "assemble",
        "update",
        "verb"
      ]
    },
    "layout": "updater",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "generate-eslint",
      "update"
    ]
  }
}
